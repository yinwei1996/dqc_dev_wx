.tip-label{padding:2rpx 5rpx;text-align:center;font-size:20rpx;border-radius:5rpx}.tip-label-primary{padding:2rpx 5rpx;text-align:center;font-size:20rpx;border-radius:5rpx;color:#fff;background-color:#f90}.tip-label-primary-outline{padding:2rpx 5rpx;text-align:center;font-size:20rpx;border-radius:5rpx;color:#f90;background-color:rgba(255,153,0,.2)}.paging-tip .no-records{padding:120rpx 0;text-align:center}.paging-tip .no-records image{width:160rpx;height:160rpx;margin-bottom:-30rpx}.paging-tip text{display:flex;justify-content:center;align-items:center;box-sizing:border-box;height:80rpx;color:#aaa}.address-item{display:flex;justify-content:space-between;align-items:center;box-sizing:border-box;padding:0 20rpx;position:relative;width:100%;height:120rpx;border-bottom:solid 1px #f0f0f0}.address-item .address-user text:first-child{font-size:30rpx;margin-right:20rpx}.address-item .address-user .is-default{padding:2rpx 5rpx;text-align:center;font-size:20rpx;border-radius:5rpx;color:#fff;background-color:#f90;margin-left:25rpx;vertical-align:5rpx}.address-item .address-location{margin-top:5rpx;font-size:22rpx}.address-item .address-del{display:flex;justify-content:center;align-items:center;box-sizing:border-box;position:absolute;right:30rpx;width:245rpx;height:65rpx;color:#fff;background-color:#f10000;z-index:2}
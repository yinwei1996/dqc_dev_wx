.tip-label{padding:2rpx 5rpx;text-align:center;font-size:20rpx;border-radius:5rpx}.tip-label-primary{padding:2rpx 5rpx;text-align:center;font-size:20rpx;border-radius:5rpx;color:#fff;background-color:#f90}.tip-label-primary-outline{padding:2rpx 5rpx;text-align:center;font-size:20rpx;border-radius:5rpx;color:#f90;background-color:rgba(255,153,0,.2)}.search-bar{display:flex;justify-content:space-between;align-items:center;padding:10rpx 15rpx;width:100%;box-sizing:border-box}.search-bar>view{display:flex;justify-content:flex-start;align-items:center;box-sizing:border-box;width:90%;background-color:#f0f0f0;border:1px solid #f0f0f0;border-radius:25rpx}.search-bar>view .endfix,.search-bar>view .prefix{width:40rpx;height:40rpx;margin:0 10rpx}.search-bar>view .ipt{display:flex;justify-content:flex-start;align-items:center;box-sizing:border-box;width:80%;height:50rpx;border:none}.search-bar>view .op{color:#333}.search-bar.readonly>view{width:100%}.search-history{position:fixed;top:100rpx;left:0;right:0;bottom:0;background-color:#fff;z-index:1}.search-history .title{position:relative;display:block;padding:0 40rpx;height:80rpx;line-height:80rpx}.search-history .title image{position:absolute;top:27rpx;right:40rpx;width:28rpx;height:28rpx}.search-history .body{padding:0 40rpx}.search-history .body text{display:inline-block;margin:0 30rpx 20rpx 0;padding:5rpx 20rpx;border-radius:5rpx;border:1px solid #ccc}
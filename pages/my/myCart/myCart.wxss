.tip-label{padding:2rpx 5rpx;text-align:center;font-size:20rpx;border-radius:5rpx}.tip-label-primary{padding:2rpx 5rpx;text-align:center;font-size:20rpx;border-radius:5rpx;color:#fff;background-color:#f90}.tip-label-primary-outline{padding:2rpx 5rpx;text-align:center;font-size:20rpx;border-radius:5rpx;color:#f90;background-color:rgba(255,153,0,.2)}.my-cart{padding-bottom:70rpx}.my-cart .cart-tip{display:flex;justify-content:flex-start;align-items:center;box-sizing:border-box;padding:0 20rpx;height:80rpx;font-size:26rpx;color:#fff;background-color:#f90}.my-cart .cart-expired{display:flex;justify-content:space-between;align-items:center;box-sizing:border-box;padding:0 20rpx;height:80rpx;border-bottom:solid 1px #f0f0f0}.my-cart .cart-expired .btn{float:left}.my-cart .cart-expired .expire-count{text-align:right}.my-cart .cart-ops{position:fixed;left:0;right:0;bottom:0;display:flex;height:80rpx;z-index:2}.my-cart .cart-ops .cart-total{display:flex;justify-content:space-between;align-items:center;padding:0 20rpx;width:70%;box-sizing:border-box;background-color:#f0f0f0}.my-cart .cart-ops .cart-total .total-qty{display:flex;justify-content:flex-start;align-items:center;box-sizing:border-box;background-color:#f0f0f0}.my-cart .cart-ops .cart-total .total-qty image{padding:15rpx 0;width:40rpx;height:40rpx}.my-cart .cart-ops .cart-total .total-qty text{margin-left:15rpx}.my-cart .cart-ops .cart-total .total-amount{display:flex;justify-content:flex-end;align-items:center;box-sizing:border-box;font-size:30rpx;color:#f90;background-color:#f0f0f0}.my-cart .cart-ops .cart-done{display:flex;justify-content:center;align-items:center;box-sizing:border-box;width:30%;font-size:30rpx;color:#fff;background-color:#f90}.my-cart .cart-ops .cart-done.disabled{color:#ccc;background-color:#aaa}.my-cart .wish-tip{display:flex;justify-content:center;align-items:center;box-sizing:border-box;height:80rpx;background-color:#f0f0f0}